{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --color",
    "format": "prettier --write src/**/*.{ts,tsx,scss,css,json}"
  },
  "dependencies": {
    "@aws-amplify/api": "^4.0.20",
    "@aws-amplify/auth": "^4.3.10",
    "@aws-amplify/core": "^4.3.2",
    "@aws-amplify/pubsub": "^4.1.12",
    "@aws-amplify/storage": "^4.4.3",
    "@expo/react-native-action-sheet": "^3.11.0",
    "@react-native-async-storage/async-storage": "~1.15.0",
    "@react-native-community/datetimepicker": "3.5.2",
    "@react-native-community/netinfo": "6.0.2",
    "@react-native-community/segmented-control": "^2.2.2",
    "@react-native-masked-view/masked-view": "0.2.5",
    "@react-navigation/bottom-tabs": "^6.0.7",
    "@react-navigation/native": "^6.0.4",
    "@react-navigation/stack": "^6.0.9",
    "expo": "^43.0.0",
    "expo-cellular": "~4.0.0",
    "expo-constants": "~12.1.3",
    "expo-image-picker": "~11.0.3",
    "expo-localization": "~11.0.0",
    "expo-location": "~13.0.4",
    "expo-sharing": "~10.0.3",
    "expo-status-bar": "~1.1.0",
    "formik": "^2.2.9",
    "i18n-js": "^3.8.0",
    "lottie-react-native": "4.0.3",
    "pinar": "^0.12.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-appearance": "~0.3.3",
    "react-native-bouncy-checkbox": "^2.1.4",
    "react-native-elements": "^3.4.2",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-input-spinner": "^1.7.11",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-maps": "0.28.0",
    "react-native-reanimated": "~2.2.0",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.8.0",
    "react-native-skeleton-placeholder": "^5.0.0",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-web": "0.17.1",
    "react-navigation-header-buttons": "^9.0.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-typescript": "^7.15.0",
    "@react-native-community/eslint-config": "^3.0.1",
    "@types/i18n-js": "^3.8.2",
    "@types/jest": "^27.0.2",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "@types/react-native-input-spinner": "^1.4.2",
    "@types/react-native-snap-carousel": "^3.8.4",
    "@types/react-test-renderer": "^17.0.1",
    "@types/yup": "^0.29.13",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "babel-plugin-inline-dotenv": "^1.6.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.2.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-native": "^3.11.0",
    "husky": "^7.0.2",
    "jest-expo": "^43.0.0",
    "prettier": "^2.4.1",
    "react-test-renderer": "^17.0.2",
    "typescript": "~4.3.5"
  },
  "private": true,
  "eslintConfig": {
    "plugins": [
      "react-native",
      "prettier",
      "@typescript-eslint"
    ],
    "extends": [
      "@react-native-community",
      "prettier",
      "plugin:jsx-a11y/strict",
      "prettier/react",
      "plugin:@typescript-eslint/recommended"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser",
      "project": "./tsconfig.json"
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {}
      ],
      "no-console": "off",
      "@typescript-eslint/no-unused-vars": "warn",
      "react-native/no-unused-styles": "warn"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm run lint"
    }
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ]
  }
}
